{
  "name": "seeds-monorepo",
  "version": "0.0.0",
  "devDependencies": {
    "@manypkg/cli": "^0.23.0",
    "prettier": "^3.3.0",
    "syncpack": "^13.0.4",
    "typescript": "^5.4.0"
  },
  "packageManager": "pnpm@9.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "check:packages": "manypkg check",
    "deps:check": "pnpm exec syncpack lint",
    "deps:fix": "pnpm exec syncpack fix-mismatches && pnpm exec syncpack format",
    "fix:packages": "manypkg fix",
    "format": "npx prettier --write \"packages/**/*.{ts,tsx,js,json}\"",
    "format:check": "npx prettier --check \"packages/**/*.{ts,tsx,js,json}\""
  },
  "type": "module"
}
