{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/api-extractor.schema.json",

  "mainEntryPointFilePath": "<projectFolder>/dist/index.d.ts",

  "apiReport": {
    "enabled": true,
    "reportFolder": "<projectFolder>/etc",
    "reportFileName": "models.api.md"
  },

  "dtsRollup": {
    "enabled": true,

    "untrimmedFilePath": "<projectFolder>/dist/models.untrimmed.d.ts",

    "alphaTrimmedFilePath": "<projectFolder>/dist/models-alpha.d.ts",

    "betaTrimmedFilePath": "<projectFolder>/dist/models-beta.d.ts",

    "publicTrimmedFilePath": "<projectFolder>/dist/models.d.ts"
  },

  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "<projectFolder>/etc/models.api.json"
  },

  "messages": {
    "extractorMessageReporting": {
      "ae-missing-release-tag": {
        "logLevel": "warning"
      }
    }
  }
}

