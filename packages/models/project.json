{
  "name": "@seeds/models",
  "sourceRoot": "packages/models/src",
  "projectType": "library",

  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "tsc --build tsconfig.build.json"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint ."
      }
    }
  }
}

