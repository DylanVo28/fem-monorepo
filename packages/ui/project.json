{
  "name": "@seeds/ui",
  "sourceRoot": "packages/ui/src",
  "projectType": "library",

  "implicitDependencies": ["@seeds/models"],

  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "tsc --build tsconfig.build.json"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo \"no tests\""
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint ."
      }
    }
  }
}

